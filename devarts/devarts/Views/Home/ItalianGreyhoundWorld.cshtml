                                <div class="col-lg-12 text-center">
                                    <h5><b>Nasze Psy <small>Our Dogs</small></b><br><small style="color:darkgoldenrod">Birthdays</small></h5>
                                    <hr />
                                    <br />
                                </div>
                                @if (Model.Where(m => m.Month == 1 && m.dog != null).Count() > 0)
                                {
                                    foreach (var month in Model.Where(m => m.Month == 1 && m.dog != null))
                                    {
                                        <div class="col-lg-3 text-center">
                                            <a style="cursor:pointer" href="/Dog/@month.dog.DogLink" target="_blank">
                                                @if (string.IsNullOrEmpty(month.dog.DeathDate))
                                                {
                                                    <img class="circular_image" alt="" src="/Dogs/@month.dog.BreedLink/@month.dog.DogLink/@month.dog.MainPicture">
                                                }
                                                else
                                                {
                                                    <img class="circular_image inMemories" alt="" src="/Dogs/@month.dog.BreedLink/@month.dog.DogLink/@month.dog.MainPicture">
                                                }
                                            </a>
                                            <h5>
                                                <small>@month.dog.BreedName</small><br />
                                                <a style="cursor: pointer" target="_blank" href="/Dog/@month.dog.DogLink">
                                                    @month.dog.DogName
                                                </a>
                                            </h5>
                                            <hr style="width:50%" />
                                            <h5>
                                                @if (month.IsPast)
                                                {
                                                    if (string.IsNullOrEmpty(month.dog.DeathDate))
                                                    {
                                                        <small>@month.BornDateInThisYear</small>
                                                    }
                                                    else
                                                    {
                                                        <small>@month.BornDateInThisYear</small>
                                                    }
                                                }
                                                else
                                                {
                                                    if (string.IsNullOrEmpty(month.dog.DeathDate))
                                                    {
                                                        <text><small>Za</small></text> @Html.Raw(month.RemainingDays) <br />
                                                        <small>@month.BornDateInThisYear</small>
                                                    }
                                                    else
                                                    {
                                                        <text><b><small>Rocznica za</small> </b></text> @Html.Raw(month.RemainingDays) <br />
                                                        <small>@month.BornDateInThisYear</small>
                                                    }
                                                }
                                            </h5>
                                        </div>
                                    }
                                }
                                else
