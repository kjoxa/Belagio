@model IEnumerable<devarts.Helpers.Birthdays>

@section metatags
{
    <meta property="og:title" content="Super charty - Urodziny chartów">
    <meta property="og:image" content="https://www.supercharty.pl/img/birthdays.jpg">
    <meta property="og:url" content="https://www.supercharty.pl/Home/ItalianGreyhoundWorld/">
    <meta property="og:description" content="Zobaczysz tu urodziny naszych chartów oraz ich miotów">
    <meta property="og:type" content="website">
    <meta name="robots" content="noodp">
}

<style>
    .circular_image {
        width: 120px;
        hseven: 100%;
        border-radius: 50%;
        overflow: hidden;
        background-color: #c1a34c;
        object-fit: cover;
        display: inline-block;
        vertical-align: middle;
        border: 3px solid #c1a34c !important;
    }

        .circular_image.litter {
            height: 150px;
        }

        .circular_image img {
            width: 100%;
        }

    .inMemories {
        -webkit-filter: grayscale(100%); /* Safari 6.0 - 9.0 */
        filter: grayscale(100%);
    }

       /* Extra Small Devices, Phones */
    @@media only screen and (max-width : 480px) {
        .col-lg-3 {
            margin-bottom: 50px !important;
        }
    }

    /* Small Devices, Tablets */
    @@media only screen and (max-width : 768px) {
        .col-lg-3{
            margin-bottom: 50px !important;
        }
    }

</style>
<div class="row">
    <div class="box">
        <div class="col-lg-12">

            <hr>
            <h2 class="text-center orange"><i class="fa fa-2x fa-paw"></i></h2>
            <h2 class="intro-text text-center">
                Lokalne życie <strong>Super Chartów</strong>
                <span class="under">
                    Sighthounds World
                </span>
            </h2>
            <hr>
        </div>
        <div class="clearfix"></div>
    </div>
</div>
