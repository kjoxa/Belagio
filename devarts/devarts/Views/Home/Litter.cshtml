<!-- TABELA SZCZENIĄT -->
<section class="content background-grey" style="padding: 30px 0 10px 0">
    <div class="container">
        <div class="belagio_container">
            <div class="col-lg-12">
                <div class="title text-center">
                    <hr class="hr-text" data-content=" SZCZENIĘTA \ Puppies">
                </div>
            </div>

            <div class="col-lg-12">
                <h5 class="text-center">Data urodzenia \ <span class="gold">Date of birth</span>
                <br />
                <b>@Model.Litter.BornDate</b></h5>
            </div>

            <div class="row" style="margin-top:20px;">
                @{
                    var puppies = Model.Puppies;
                    int i = 0;
                }
                @if (Request.Browser.IsMobileDevice)
                {
                    <div class="col-lg-12">
                        <style>
                            .gold {
                                color: rgba(170, 150, 91)
                            }
                            .white {
                                color: white;
                            }

                            a {
                                color: rgba(170, 150, 91);
                            }
                        </style>
                        <table class="table table-striped white">
                            <thead>
                                <tr>
                                    <th>Imię<br /><small class="gold">Name</small></th>
                                    <th>Płeć<br /><small class="gold">Sex</small></th>
                                    @*<th>Tytuły<br /><small class="gold">Degree</small></th>
                                <th>Uwagi<br /><small class="gold">Description</small></th>*@
                                    <th>Miejsce<br /><small class="gold">Home</small></th>
                                    <th>Czas życia<br /><small class="gold">Lifetime</small></th>
                                </tr>
                            </thead>
                            <tbody>

                                @if (puppies.Count() > 0)
                                {
                                    var _age = new AgeHelper();
                                    foreach (var puppy in Model.Puppies)
                                    {
                                        <tr>
                                            <td><b><a href="javascript:void(0);">@puppy.PuppyName</a></b></td>
                                            <td>@(puppy.PuppySex == false ? Html.Raw("<b><i class='fa fa-venus'></i></b>") : Html.Raw("<b><i class='fa fa-mars'></i></b>"))</td>
                                            @*<td><small>@puppy.PuppyAchievements</small></td>
                                        <td><small>@Html.Raw(puppy.Description)</small></td>*@
                                            <td><small>@puppy.Country @(string.IsNullOrEmpty(puppy.City) ? "" : puppy.City)</small></td>
                                            @if (string.IsNullOrEmpty(puppy.DeathDate))
                                            {
                                                <td>@puppy.BornDate <small>@_age.HowOld(Model.Litter.BornDate, null)</small></td>

                                            }
                                            else
                                            {
                                                <td>
                                                    <small>
                                                        <b>@Model.Litter.BornDate - @puppy.DeathDate</b>
                                                    </small><br />
                                                    <small>@_age.HowOld(Model.Litter.BornDate, puppy.DeathDate)</small>
                                                    <br />
                                                    <small>@puppy.CauseOfDeath</small>
                                                </td>
                                            }
                                        </tr>

                                    }
                                }
                                else
                                {
                                    <h4 class="text-center">Do tego miotu nie są dodane jeszcze żadne szczenięta.</h4>
                                }


                            </tbody>
                        </table>
                    </div>
                }
                else
                {
                    <div class="col-lg-12">
                        <style>
                            .gold {
                                color: rgba(170, 150, 91)
                            }

                            .white {
                                color: white;
                            }

                            a {
                                color: rgba(170, 150, 91);
                            }
                        </style>
                        <table class="table table-striped white">
                            <thead>
                                <tr>
                                    <th>Imię<br /><small class="gold">Name</small></th>
                                    <th>Płeć<br /><small class="gold">Sex</small></th>
                                    <th>Opis<br /><small class="gold">Description</small></th>
                                    <th>Miejsce zamieszkania<br /><small class="gold">Home</small></th>
                                    <th>Czas życia<br /><small class="gold">Life time</small></th>
                                </tr>
                            </thead>
                            <tbody>

                                @if (puppies.Count() > 0)
                                {
                                    var _age = new AgeHelper();
                                    foreach (var puppy in Model.Puppies)
                                    {
                                        <tr>
                                            <td><b><a href="javascript:void(0);">@puppy.PuppyName</a></b></td>
                                            <td>@(puppy.PuppySex == false ? Html.Raw("<b><i class='fa fa-venus'></i></b>") : Html.Raw("<b><i class='fa fa-mars'></i></b>"))</td>
                                            <td><small>@puppy.PuppyAchievements</small><br /><small>@Html.Raw(puppy.Description)</small></td>
                                            <td><small>@puppy.Country @(string.IsNullOrEmpty(puppy.City) ? "" : puppy.City)</small></td>
                                            @if (string.IsNullOrEmpty(puppy.DeathDate))
                                            {
                                                <td>@puppy.BornDate <small>@_age.HowOld(Model.Litter.BornDate, null)</small></td>

                                            }
                                            else
                                            {
                                                <td>
                                                    <small>
                                                        <b>@Model.Litter.BornDate - @puppy.DeathDate</b>
                                                    </small><br />
                                                    <small>@_age.HowOld(Model.Litter.BornDate, puppy.DeathDate)</small>
                                                    <br />
                                                    <small>@puppy.CauseOfDeath</small>
                                                </td>
                                            }
                                        </tr>

                                    }
                                }
                                else
                                {
                                    <h4 class="text-center">Do tego miotu nie są dodane jeszcze żadne szczenięta.</h4>
                                }

                            </tbody>
                        </table>
                    </div>
                }
            </div>
        </div>
    </div>
</section>

<!-- GALERIA SZCZENIĄT -->
<section class="content background-grey" style="padding: 30px 0 30px 0">
    <div class="container">
        <div class="belagio_container">
            <div class="col-lg-12">
                <div class="title text-center">
                    <hr class="hr-text" data-content=" SZCZENIĘTA \ Puppies ">
                </div>
            </div>
            <div class="col-lg-12">
                <section id="page-content" class="belagio-gallery">
                    <div class="container">
                        <div class="grid-layout grid-4-columns" data-margin="20" data-item="grid-item" data-lightbox="gallery">
                            <div class="row">
                                @if (Model.Puppies.Count() > 0)
                                {

                                    i = 0;
                                    var _litterRepo = new devarts.Repositories.KennelRepository();
                                    puppies = _litterRepo.GetPuppiesByLitterLink(Model.Litter.LitterLink);
                                    foreach (var puppy in puppies)
                                    {
                                        if (i != 0 && i % 4 == 0)
                                        {
                                        @:</div><br />
                                        @:<div class="row">
                                        }
                                        <div class="grid-item">
                                            <p class="gold">@(puppy.PuppySex == false ? Html.Raw("<b><i class='fa fa-venus'></i></b>") : Html.Raw("<b><i class='fa fa-mars'></i></b>")) @puppy.PuppyName</p>
                                            <a class="image-hover-zoom" href="~/Puppies/@puppy.BreedLink/@puppy.PuppyLink/@puppy.MainPicture" data-lightbox="gallery-image">
                                                <img src="~/Puppies/@puppy.BreedLink/@puppy.PuppyLink/@puppy.MainPicture">
                                            </a>
                                        </div>
                                        i++;
                                    }
                                }
                                else
                                {
                                    <div class="col-lg-12 text-center">
                                        <p>
                                            Do tej sekcji nie dodano jeszcze informacji.<br />
                                            <small class="gold">Information hasn't been done to this section yet.</small>
                                        </p>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</section>

