@Styles.Render("~/Content/Features/Pedigree/pedigree.css")
@model devarts.Models.DogAndLitterAndImagesAndPuppies

<!-- OGÓLNE -->
<section class="content background-grey" style="padding: 30px 0 10px 0">
    <div class="container">
        <div class="belagio_container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="title text-center">
                        <hr class="hr-text" data-content=" MIOT \ @Model.Litter.LitterPresentationName ">
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-12" style="display: inline-flex">
                    <div class="col-lg-6">
                        <h4 class="text-center">Matka <small>Mother</small></h4>
                        <img class="picture-border" style="width:100%" src="~/Dogs/@Model.dog.BreedLink/@Model.dog.DogLink/@Model.dog.MainPicture">
                        <hr />
                        <p style="color: rgba(170, 150, 91)">@Html.Raw(Model.Litter.MotherLitterDescription)</p>
                    </div>
                    <div class="col-lg-6">
                        <h4 class="text-center">Ojciec <small>Father</small></h4>
                        <img class="picture-border" style="width:100%" src="~/Litters/@Model.Litter.BreedLink/@Model.Litter.LitterLink/@Model.Litter.FatherPictureName">
                        <hr />
                        <p style="color: rgba(170, 150, 91)">@Html.Raw(Model.Litter.FatherLitterDescription)</p>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-12 text-center">
                    <br />
                    <p class="silver">@Html.Raw(Model.Litter.Description)</p>
                </div>
            </div>
        </div>
    </div>
</section>

