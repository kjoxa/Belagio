<div class="row">
    <div class="col-md-4">
        @{
            const string Canvas = "Cieczki";
            var complexChart = new LineChart();
            var data = new FakeComplexChartData();
            complexChart.ComplexData.Labels.AddRange(data.Labels);
            complexChart.ComplexData.Datasets.AddRange(data.Datasets);
        }
        <h3>@Canvas</h3>
        <canvas id="@Canvas" width="400" height="400"></canvas>
        @Html.CreateChart(Canvas, complexChart)
    </div>

    <div class="col-md-4">
        @{
            const string CanvasB = "Wiek";
            var complexChartB = new BarChart();
            //var data = new FakeComplexChartData();
            complexChartB.ComplexData.Labels.AddRange(data.Labels);
            complexChartB.ComplexData.Datasets.AddRange(data.Datasets);
        }
        <h3>Wiek psów w hodowli</h3>
        <canvas id="@CanvasB" width="400" height="400"></canvas>
        @Html.CreateChart(CanvasB, complexChartB)
    </div>

